{
  // ============================================================================
  // MANUAL CAPTURE TEMPLATE (Structured)
  // ============================================================================
  // Purpose: Bootstrap structured data capture while building automation
  // Status: Temporary - will be migrated to permanent structure
  //
  // Copy this template and rename: YYYY-MM-DD_type_description.jsonc
  // Fill in relevant fields for your manual capture

  "$schema": "../../../../../system/config/schemas/temp/manual-capture.schema.json",

  // ============================================================================
  // METADATA - What Are You Capturing?
  // ============================================================================

  "capture_metadata": {
    "date": "2025-01-01",
    "type": "session-notes",           // session-notes | activity-log | pattern-observation | sleep-tracking | etc
    "description": "Brief description of what this captures",
    "captured_by": "user-id or instance-id",
    "automation_target": "Where this data will eventually live"
  },

  // ============================================================================
  // DATA - The Actual Information
  // ============================================================================
  // Structure this however makes sense for what you're capturing
  // Examples below - use what's relevant, delete what's not

  "data": {
    // Example 1: Session activity
    "session_id": "session-identifier",
    "activities": [
      {
        "timestamp": "HH:MM:SS",
        "activity": "What happened",
        "context": "Additional details"
      }
    ],

    // Example 2: Sleep/downtime tracking
    "sleep_periods": [
      {
        "start": "2025-01-01T00:00:00Z",
        "end": "2025-01-01T08:00:00Z",
        "duration_minutes": 480,
        "notes": "Example sleep period"
      }
    ],

    // Example 3: Pattern observations
    "patterns_observed": [
      {
        "pattern": "Description of pattern",
        "evidence": "What showed this pattern",
        "frequency": "How often observed",
        "confidence": "emerging"           // emerging | establishing | validated
      }
    ],

    // Example 4: Work context
    "work_context": {
      "project": "project-id",
      "workflow": "build-first | plan-then-build | etc",
      "focus": "What was being worked on",
      "outcomes": "What was accomplished"
    },

    // Example 5: Generic key-value
    "observations": [
      {
        "timestamp": "YYYY-MM-DD HH:MM:SS",
        "observation": "What you noticed",
        "significance": "Why this matters"
      }
    ]
  },

  // ============================================================================
  // MIGRATION NOTES
  // ============================================================================

  "migration": {
    "destination": "Where this should be migrated to",
    "format_changes": "What needs to be transformed",
    "validation": "How to verify migration was successful",
    "dependencies": "What needs to exist before migration"
  },

  // ============================================================================
  // NOTES
  // ============================================================================

  "notes": [
    "Any additional context",
    "Things to remember during migration",
    "Why certain fields are structured this way"
  ]
}
