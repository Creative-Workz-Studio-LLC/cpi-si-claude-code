{
  // ============================================================================
  // ARCHIVE TEMPLATE - Processed and Released
  // ============================================================================
  // Purpose: Searchable history without active memory load
  // Lifecycle: Permanent but dormant (retrievable if needed)
  // Biblical Foundation: Releasing what's processed, keeping searchable record
  //
  // STRUCTURE PRINCIPLE:
  // - Core required fields (session identification)
  // - Optional standard fields (summary and connections)
  // - Extensions (learning what archives need)

  "$schema": "../../../../config/schemas/session/archive.schema.json",

  // ============================================================================
  // CORE (REQUIRED) - Must Exist
  // ============================================================================

  "archive_id": "session-id",                  // Original session identifier
  "instance_id": "nova_dawn",
  "user_id": "seanje-lenox-wise",
  "project_id": "project-id",                  // Which project (from session)
  "date": "2025-01-01",
  "archived_at": "2025-01-01T12:00:00Z",       // When this was archived

  // ============================================================================
  // OPTIONAL STANDARD FIELDS - Agency
  // ============================================================================

  "duration_minutes": 0,
  "context": "Brief summary",
  "work_context": "/path/to/workspace",        // From session → project

  "session_metadata": {
    // Key context from session (for searchability)
    "workflow_used": "build-first",            // From session → project → instance
    "project_type": "system_architecture",     // From session → project
    "circadian_phase": "morning",              // From session → user schedule
    "quality_indicators": {
      "tasks_completed": 0,
      "breakthroughs": 0,
      "struggles": 0
    }
  },

  "processed_into": {
    "significant_memories": [],                // Memory IDs created from this session
    "patterns_contributed": [],                // Pattern IDs this session informed
    "lessons_extracted": []                    // Lesson IDs drawn from this session
  },

  "searchable_summary": "High-level description for retrieval if needed",
  "tags": [],                                  // For searchability

  "retrieval_metadata": {
    "project_name": "",                        // Human-readable project name
    "project_phase": "",                       // Which development phase
    "significance_level": "routine"            // routine | notable | formative
  },

  // ============================================================================
  // EXTENSIONS - Discovery Space
  // ============================================================================

  "extensions": {
    // What else do we need for good archival retrieval
  }
}
