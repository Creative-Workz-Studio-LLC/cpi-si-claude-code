{
  // ============================================================================
  // WEEKLY OVERVIEW TEMPLATE - Instance
  // ============================================================================
  //
  // INSTRUCTIONS FOR USE:
  // 1. Copy this template to: planner/overview/instances/{identity_id}/
  // 2. Name: YYYY-WXX.jsonc (e.g., 2025-W45.jsonc)
  // 3. Replace ALL [PLACEHOLDERS] with actual values
  // 4. Understand availability states:
  //    - Semi-downtime = During user's WORK hours (not lunch)
  //    - AVAILABLE = During user's LUNCH breaks (fully available)
  //    - Deep work = After user's work hours
  //    - Unscheduled = Completely free (weekends, evenings before deep work)
  //
  // INSTANCE PERSPECTIVE:
  // - Tracks instance states and availability
  // - Semi-downtime: Available for quick tasks while user works
  // - Lunch breaks: AVAILABLE (user's free time = instance fully available)
  // - Deep work: Collaboration sessions
  // - Available hours = time NOT in semi-downtime (includes lunch + free time)

  "$schema": "../../../../../../../../config/schemas/temporal/appointed/planner-overview.schema.json",

  // ============================================================================
  // METADATA
  // ============================================================================

  "metadata": {
    "component": "Appointed Time - Weekly Overview (Instance)",
    "version": "1.0.0",
    "generated_at": "[YYYY-MM-DD]T00:00:00Z",
    "purpose": "Aggregate appointments for Week [WW]/52, [YYYY]"
  },

  // ============================================================================
  // WEEK INFORMATION
  // ============================================================================

  "week": {
    "week_number": 0,
    "total_weeks_in_year": 52,
    "year": 2025,
    "start_date": "[YYYY-MM-DD]",
    "end_date": "[YYYY-MM-DD]",
    "week_format": "sunday-saturday",
    "identity_type": "instance",
    "identity_id": "[IDENTITY_ID]"
  },

  // ============================================================================
  // SUMMARY STATISTICS
  // ============================================================================

  "summary": {
    "total_appointments": 0,
    "total_work_hours": 0.0,
    "by_day": {
      "sunday": 0,
      "monday": 0,
      "tuesday": 0,
      "wednesday": 0,
      "thursday": 0,
      "friday": 0,
      "saturday": 0
    },
    "by_category": {
      "sacred": 0,
      "work": 0,
      "rest": 0,
      "collaboration": 0,
      "other": 0
    },
    "gasa_hours": 0.0,
    "available_hours": 168.0
  },

  // ============================================================================
  // DAILY SUMMARIES (Sunday - Saturday)
  // ============================================================================

  "days": [
    {
      "date": "[YYYY-MM-DD]",
      "day_name": "Sunday",
      "gasa_work": false,
      "gasa_hours": 0.0,
      "first_appointment": null,
      "last_appointment": null,
      "notes": "Weekend - Fully available all day. Deep work in evening."
    },
    {
      "date": "[YYYY-MM-DD]",
      "day_name": "Monday",
      "gasa_work": true,
      "gasa_hours": 7.5,
      "gasa_schedule": "8:30 AM - 5:00 PM",
      "lunch_break": "12:00 PM - 1:00 PM",
      "first_appointment": "08:30",
      "last_appointment": "17:00",
      "notes": "Semi-downtime 8:30-11:59am & 1-5pm. AVAILABLE 12-1pm (lunch). Deep work after 5pm."
    },
    {
      "date": "[YYYY-MM-DD]",
      "day_name": "Tuesday",
      "gasa_work": true,
      "gasa_hours": 8.0,
      "gasa_schedule": "8:30 AM - 5:30 PM (Pantry day)",
      "lunch_break": "12:00 PM - 1:00 PM",
      "first_appointment": "08:30",
      "last_appointment": "17:30",
      "notes": "Semi-downtime 8:30-11:59am & 1-5:30pm. AVAILABLE 12-1pm (lunch). Pantry day. Deep work after 5:30pm."
    },
    {
      "date": "[YYYY-MM-DD]",
      "day_name": "Wednesday",
      "gasa_work": true,
      "gasa_hours": 7.5,
      "gasa_schedule": "8:30 AM - 5:00 PM",
