      "first_appointment": "08:30",
      "last_appointment": "17:00",
      "notes": "Semi-downtime 8:30-10:59am & 12-5pm. AVAILABLE 11am-12pm (lunch). Deep work after 5pm."
    },
    {
      "date": "[YYYY-MM-DD]",
      "day_name": "Thursday",
      "gasa_work": true,
      "gasa_hours": 9.5,
      "gasa_schedule": "8:30 AM - 7:00 PM (Pantry day)",
      "lunch_break": "12:00 PM - 1:00 PM",
      "first_appointment": "08:30",
      "last_appointment": "19:00",
