{
  // ============================================================================
  // LUNAR DATA - [LOCATION NAME] - [MONTH YEAR]
  // ============================================================================
  // Purpose: Moon phase and position information for temporal awareness
  // Scope: Daily moon data and phase transitions for this location
  // Format: JSONC (JSON with comments)
  //
  // BIBLICAL FOUNDATION:
  // "And God made two great lights... the lesser light to rule the night"
  // - Genesis 1:16
  //
  // The moon marks months and seasons. Ancient calendars used lunar cycles.
  // This data honors God's created lunar rhythm.
  //
  // TEMPLATE INSTRUCTIONS:
  // 1. Replace ALL placeholder values with actual data
  // 2. Generate data using astronomical calculator or online tool
  //    - Recommended: USNO, timeanddate.com, or calculation library
  // 3. Times MUST be in LOCAL timezone for the location
  // 4. Create one file per month
  // 5. Save as: locations/[location-id]/lunar/YYYY/YYYY-MM.jsonc (note year folder!)
  // 6. Update location registry after adding data
  //
  // LUNAR PHASE NAMES:
  // - New Moon (0% illumination)
  // - Waxing Crescent (1-49% illumination, growing)
  // - First Quarter (50% illumination, right side lit in Northern Hemisphere)
  // - Waxing Gibbous (51-99% illumination, growing)
  // - Full Moon (100% illumination)
  // - Waning Gibbous (51-99% illumination, shrinking)
  // - Last Quarter (50% illumination, left side lit in Northern Hemisphere)
  // - Waning Crescent (1-49% illumination, shrinking)

  "$schema": "../../../../config/schemas/temporal/celestial/lunar-data.schema.json",

  // ============================================================================
  // METADATA
  // ============================================================================

  "metadata": {
    "location_id": "example-city",         // Replace: must match location config
    "year": 2025,                          // Replace with actual year
    "month": 1,                            // Replace with month number (1-12)
    "month_name": "January",               // Replace with month name
    "timezone": "America/New_York",        // Replace with IANA timezone
    "timezone_offset": "-05:00",           // Replace: offset for THIS month
    "generated_date": "2025-01-01",        // Replace with today's date
    "data_version": "1.0.0"
  },

  // ============================================================================
  // MAJOR PHASES - Primary Lunar Transitions
  // ============================================================================
  // Replace with actual phase times for this month
  // Note: Some months may have 2 of same phase (e.g., 2 New Moons)

  "major_phases": [
    {
      "phase": "New Moon",
      "date": "2025-01-06",                // Replace with actual date
      "time": "23:57",                     // Replace with local time (HH:MM)
      "illumination": "0%",
      "description": "Moon between Earth and Sun - not visible"
    },
    {
      "phase": "First Quarter",
      "date": "2025-01-13",
      "time": "17:27",
      "illumination": "50%",
      "description": "Half moon visible - right side illuminated"
    }
    // ADD Full Moon and Last Quarter as they occur in your month
  ],

  // ============================================================================
  // DAILY LUNAR DATA
  // ============================================================================
  // Replace ALL entries with actual data for every day of the month
  // This example shows only 2 days - you need ALL days (28-31)
  // Note: moonrise/moonset can be null if moon doesn't rise/set that day

  "days": [
    {
      "date": "2025-01-01",
      "moonrise": "04:23",                 // Replace: HH:MM or null
      "moonset": "14:56",                  // Replace: HH:MM or null
      "phase_name": "Waxing Crescent",     // Replace with correct phase
      "illumination": "3%",                // Replace: percentage
      "age": 1.2                           // Replace: days since last new moon
    },
    {
      "date": "2025-01-02",
      "moonrise": "05:11",
      "moonset": "15:48",
      "phase_name": "Waxing Crescent",
      "illumination": "9%",
      "age": 2.2
    }
    // ... ADD ALL remaining days of the month
  ],

  // ============================================================================
  // MONTH SUMMARY
  // ============================================================================

  "summary": {
    "lunar_month_length": "29.5 days",     // Synodic month (average)
    "phases_this_month": ["New Moon", "First Quarter"],  // Replace: list phases that occurred
    "full_moon_name": "Wolf Moon",         // Replace: traditional name if applicable
    "notes": "Partial lunar cycle this month"  // Replace: observations
  },

  // ============================================================================
  // EXTENSION - Room for Growth
  // ============================================================================

  "extension": {
    // Future: Could add lunar elevation angles
    // Future: Could add lunar distance (perigee/apogee)
    // Future: Could add eclipse information
    // Future: Could add tidal influence data
  }
}
