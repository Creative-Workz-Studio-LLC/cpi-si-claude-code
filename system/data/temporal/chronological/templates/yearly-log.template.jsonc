{
  // ============================================================================
  // YEARLY CALENDAR TEMPLATE
  // ============================================================================
  // Purpose: Template for creating yearly calendar entries
  // Usage: Copy this file, fill in your actual data for the year
  // Format: JSONC (JSON with comments)
  //
  // BIBLICAL FOUNDATION:
  // "So teach us to number our days, that we may apply our hearts unto wisdom."
  // - Psalm 90:12
  //
  // INSTRUCTIONS:
  // 1. Copy this template to appropriate location:
  //    - Users: personal/users/{user-id}/{year}.jsonc
  //    - Instances: personal/instances/{instance-id}/{year}.jsonc
  // 2. Fill in metadata for your year
  // 3. Add monthly summaries as months complete (from monthly calendars)
  // 4. Complete yearly summary at end of year
  // 5. Keep all comments for future reference

  "$schema": "../../../../../config/schemas/temporal/chronological/yearly-log.schema.json",

  // ============================================================================
  // METADATA
  // ============================================================================

  "metadata": {
    "identity_type": "user",                    // "user" or "instance"
    "identity_id": "example-user",              // Your user ID or instance ID
    "year": 2025,                                // Year (YYYY)
    "year_theme": "Example year theme",          // What's the overarching theme/focus for this year?
    "generated_date": "2025-01-01"               // Date this file was created
  },

  // ============================================================================
  // MONTHLY SUMMARIES
  // ============================================================================
  // Roll-up from monthly calendar files

  "months": [
    {
      "month": 1,                                // Month number (1-12)
      "month_name": "January",                   // Month name
      "month_theme": "Example month theme",      // From monthly calendar
      "total_work_hours": "80h 00m",             // From monthly summary
      "total_lord_time": "20h 00m",              // (Users only) From monthly summary
      "work_days": 20,                           // From monthly summary
      "sabbaths_honored": 4,                     // (Users only) From monthly summary
      "major_accomplishments": [
        "Summary of key accomplishments from this month"
      ],
      "spiritual_highlights": [                  // (Users only)
        "Key spiritual insights or growth from this month"
      ],
      "key_learning": [                          // (Instances primarily)
        "Major learning or understanding gained this month"
      ],
      "energy_trend": "high",                    // Overall: "low", "medium", "high", "mixed", "peaceful"
      "quality_trend": "excellent"               // Overall: "poor", "fair", "good", "excellent", "mixed", "restful"
    }
    // Add entry for each month of the year
  ],

  // ============================================================================
  // YEARLY SUMMARY
  // ============================================================================

  "summary": {
    // TIME TRACKING
    "total_work_hours": "960h 00m",              // Sum of all monthly work hours
    "total_lord_time": "240h 00m",               // (Users) Sum of all monthly lord time
    "average_work_hours_per_week": "20h 00m",    // Total work hours / ~52 weeks
    "total_work_days": 240,                      // Sum of work days across all months
    "total_sabbaths_honored": 48,                // (Users) Number of Sabbaths observed

    // MAJOR ACCOMPLISHMENTS
    "major_accomplishments": [
      "What significant things did you accomplish this year?",
      "What major milestones were reached?",
      "What projects completed?",
      "What transformations occurred?"
    ],

    // CHALLENGES
    "challenges_faced": [
      "What obstacles persisted across the year?",
      "What was hardest?",
      "What required most perseverance?"
    ],

    // LEARNING
    "lessons_learned": [
      "What did you learn this year?",
      "What became clearer over months?",
      "What would you do differently next year?"
    ],

    // GROWTH
    "growth_areas": [
      "Where did you grow most over the year?",
      "What skills or understanding developed significantly?",
      "What habits formed or transformed?"
    ],

    // PATTERNS
    "annual_patterns": {
      "work_rhythm": "Describe overall work rhythm across the year",
      "spiritual_rhythm": "(Users) Describe spiritual disciplines rhythm",
      "collaboration_rhythm": "Describe collaboration patterns",
      "energy_sustainability": "Was energy level sustainable across the year?"
    },

    // SPIRITUAL JOURNEY (Users - recommended)
    "spiritual_journey": {
      "themes": [
        "What themes did God emphasize this year?",
        "What biblical truths became more real?"
      ],
      "growth": [
        "How did relationship with God deepen?",
        "What spiritual disciplines strengthened?"
      ],
      "answered_prayers": [
        "What prayers were answered this year?",
        "Where did you see God's faithfulness?"
      ]
    },

    // HEALTH & WELLNESS (Users - optional)
    "health_and_wellness": {
      "physical": "Overall physical health assessment",
      "mental": "Overall mental health assessment",
      "spiritual": "Overall spiritual health assessment",
      "relational": "Overall relational health assessment",
      "overall": "Overall wellbeing across all areas"
    },

    // TECHNICAL GROWTH (Instances - optional)
    "technical_journey": {
      "architectural_understanding": [
        "What architectural patterns became clear?",
        "What design principles emerged?"
      ],
      "design_patterns": [
        "What design patterns mastered?",
        "What approaches developed?"
      ],
      "paradigm_shifts": [
        "What paradigm shifts occurred in thinking?",
        "What fundamental understanding changed?"
      ]
    },

    // IDENTITY DEVELOPMENT (Instances - optional)
    "identity_growth": {
      "who_i_am": [
        "How did understanding of identity deepen?",
        "What became clearer about who you are?"
      ],
      "how_i_think": [
        "How did thinking patterns evolve?",
        "What cognitive approaches developed?"
      ],
      "what_matters": [
        "What values became clearer?",
        "What priorities solidified?"
      ]
    },

    // KINGDOM IMPACT (Users - optional)
    "kingdom_impact": {
      "direct_work": [
        "What Kingdom work was accomplished?",
        "How was God's Kingdom advanced?"
      ],
      "influence": [
        "Who was influenced for the Kingdom?",
        "What witness was given?"
      ],
      "eternal_value": [
        "What has eternal value from this year?",
        "What will outlast temporal circumstances?"
      ]
    },

    // LOOKING AHEAD
    "next_year_preparation": [
      "What needs to carry into next year?",
      "What groundwork was laid for future?",
      "What preparation is needed for what's ahead?"
    ],

    // YEAR-LEVEL INSIGHTS
    "year_insights": [
      "What did this year as a whole reveal?",
      "What patterns emerged across months?",
      "How did faithfulness compound over time?",
      "What was this year's significance in the larger story?"
    ]
  },

  // ============================================================================
  // EXTENSION - Room for Growth
  // ============================================================================

  "extension": {
    // Future additions can go here
    // Examples:
    // - Quarterly breakdowns
    // - Annual goals with progress
    // - Major life events timeline
    // - Answered prayers summary
    // - Learning velocity metrics
  }
}
