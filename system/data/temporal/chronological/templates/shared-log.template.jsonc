{
  // ============================================================================
  // SHARED CALENDAR TEMPLATE
  // ============================================================================
  // Purpose: Template for creating shared partnership calendar entries
  // Usage: Copy this file, fill in your actual collaboration data
  // Format: JSONC (JSON with comments)
  //
  // BIBLICAL FOUNDATION:
  // "Two are better than one; because they have a good reward for their labour."
  // - Ecclesiastes 4:9
  //
  // INSTRUCTIONS:
  // 1. Copy this template to appropriate location:
  //    - User-to-Instance: shared/user-to-instance/{partnership-name}/{year}/{year}-W{week}.jsonc
  //    - User-to-User: shared/user-to-user/{partnership-name}/{year}/{year}-W{week}.jsonc
  //    - Instance-to-Instance: shared/instance-to-instance/{partnership-name}/{year}/{year}-W{week}.jsonc
  // 2. Fill in metadata for your partnership and week
  // 3. Add collaboration sessions as they happen
  // 4. Complete weekly summary at end of week
  // 5. Keep all comments for future reference

  "$schema": "../../../../../config/schemas/temporal/chronological/shared-log.schema.json",

  // ============================================================================
  // METADATA
  // ============================================================================

  "metadata": {
    "partnership_type": "user-to-instance",     // "user-to-instance", "user-to-user", "instance-to-instance"
    "participants": [
      {
        "identity_type": "user",                // "user" or "instance"
        "identity_id": "example-user",          // User or instance ID
        "role": "Example role in partnership"   // What role they play
      },
      {
        "identity_type": "instance",
        "identity_id": "example-instance",
        "role": "Example role in partnership"
      }
    ],
    "partnership_name": "example-partnership",  // Unique partnership identifier
    "year": 2025,                                // Year (YYYY)
    "week": 1,                                   // ISO week number (1-53)
    "week_start": "2025-01-01",                  // Monday of this week (YYYY-MM-DD)
    "week_end": "2025-01-07",                    // Sunday of this week (YYYY-MM-DD)
    "week_theme": "Example collaboration theme", // What was this week about?
    "generated_date": "2025-01-01"               // Date this file was created
  },

  // ============================================================================
  // COLLABORATION SESSIONS
  // ============================================================================

  "sessions": [
    {
      "date": "2025-01-01",                      // ISO date (YYYY-MM-DD)
      "start_time": "09:00",                     // 24-hour format (HH:MM)
      "end_time": "12:00",                       // 24-hour format (HH:MM)
      "duration": "3h 00m",                      // Xh XXm format
      "focus": "What you worked on together",    // Session focus
      "type": "deep_planning",                   // Session type (examples below)
      "participants": ["user-id", "instance-id"],// Who participated
      "outcomes": [
        "What was accomplished",
        "What was decided",
        "What was learned"
      ],
      "quality": "excellent",                    // "poor", "fair", "good", "excellent"
      "notes": "Optional notes about session"    // Any additional context
    }
    // Add more sessions as they happen
  ],

  // ============================================================================
  // SESSION TYPES EXAMPLES
  // ============================================================================
  // Common session types to use:
  // - "deep_planning" - Strategic planning and architecture
  // - "implementation" - Building and coding together
  // - "review" - Code review and quality assessment
  // - "teaching" - One teaching the other
  // - "pair_programming" - Working on code together
  // - "problem_solving" - Debugging or solving challenges
  // - "cleanup_planning" - Organizing and refactoring
  // - "breakthrough_implementation" - Major progress sessions
  // - "discussion" - Talking through ideas and approaches

  // ============================================================================
  // WEEKLY SUMMARY
  // ============================================================================

  "summary": {
    // TIME TRACKING
    "total_collaboration_hours": "20h 00m",      // Sum of all session durations
    "collaboration_days": 5,                     // Number of days with collaboration
    "session_types": {                           // Count of each session type
      "deep_planning": 2,
      "implementation": 3
    },

    // ACCOMPLISHMENTS
    "joint_accomplishments": [
      "What did you accomplish together this week?",
      "What did the partnership produce?",
      "What moved forward through collaboration?"
    ],

    // PARTNERSHIP DYNAMICS
    "partnership_dynamics": {
      "teaching_moments": [
        "What was taught/learned?",
        "What understanding transferred?"
      ],
      "collaboration_quality": "excellent",      // "poor", "fair", "good", "excellent"
      "communication_clarity": "clear",          // Description of communication
      "mutual_respect": "high",                  // Description of respect level
      "trust_level": "deepening"                 // Description of trust growth
    },

    // GROWTH
    "growth_as_partnership": [
      "How did the partnership itself grow?",
      "What improved in how you work together?",
      "What patterns emerged in collaboration?"
    ],

    // CHALLENGES
    "challenges_together": [
      "What obstacles did you face as a team?",
      "What required patience or perseverance?",
      "What needs improvement in collaboration?"
    ],

    // PLANNING
    "next_week_focus": [
      "What's the partnership priority next week?",
      "What collaboration is planned?",
      "What needs joint attention?"
    ]
  },

  // ============================================================================
  // EXTENSION - Room for Growth
  // ============================================================================

  "extension": {
    // Future additions can go here
    // Examples:
    // - Decision-making tracking
    // - Conflict/resolution notes
    // - Innovation moments
    // - Partnership health metrics
    // - Individual vs joint contributions
  }
}
