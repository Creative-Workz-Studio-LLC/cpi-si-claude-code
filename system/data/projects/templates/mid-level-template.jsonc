{
  // ============================================================================
  // PROJECT TEMPLATE: MID LEVEL (Subsystem)
  // ============================================================================
  // Use this template for weeks-to-months major subsystems
  // Scope: Significant components, clear deliverables, multiple low-level tasks
  //
  // STRUCTURE PRINCIPLE:
  // - Core required fields (created order - must exist)
  // - Optional standard fields (agency - use what serves)
  // - Extensions (structured discovery space)
  // - Schema version (track evolution)

  "$schema": "../../../config/schemas/projects/project.schema.json",

  // ============================================================================
  // CORE (REQUIRED) - The Created Order
  // ============================================================================

  "schema_version": "1.0.0",
  "project_id": "unique-identifier",           // kebab-case, descriptive
  "name": "Project Name",                      // Human-readable
  "level": "mid",                              // Always "mid" for this template
  "parent_id": "parent-project-id",            // Usually references high-level project
  "status": "active",                          // planned | active | paused | completed

  "identity": {
    // REQUIRED: Who owns and works on this project
    "primary_user_id": "user-id",              // From user config
    "primary_instance_id": "instance-id",      // From instance config
    "collaborators": [
      // Additional users/instances (if applicable)
      // {"id": "collaborator-id", "type": "user|instance", "role": "description"}
    ]
  },

  "biblical_foundation": {
    "scripture": "Book Chapter:Verse",
    "text": "Actual Scripture text here",
    "principle": "How this Scripture illuminates THIS specific work - not decoration, but genuine connection between biblical truth and the purpose of this project"
  },

  // ============================================================================
  // OPTIONAL STANDARD FIELDS - Agency Within Constraints
  // ============================================================================
  // Use what serves the project - not all fields required

  "workspace": {
    "path": "/path/to/workspace",              // Primary workspace path
    "repository_url": null,                    // Git repository URL (null if not in repo)
    "branch": "main"                           // Primary branch
  },

  "inherited_preferences": {
    // Preferences inherited from user/instance configs
    "workflow": "build-first",                 // From instance config: build-first | plan-then-build | iterative
    "thinking_style": "building-blocks",       // From instance config: building-blocks | systems-thinking | sequential
    "session_defaults": {
      "thinking_enabled": false,               // Default from instance preferences
      "plan_mode": false,                      // Default from instance preferences
      "auto_accept": false                     // Default from user preferences
    },
    "work_hours": {
      "timezone": "America/Chicago",           // From user config
      "preferred_times": ["morning", "evening"] // From user config
    }
  },

  "project_type": "system_architecture",       // Relates to instance domain_expertise: game_development | system_architecture | documentation | research

  "technical_context": {
    "languages": ["go", "rust", "markdown"],   // Programming languages used
    "frameworks": [],                          // Frameworks if applicable
    "tools": ["git", "claude-code"]            // Development tools
  },

  "scope": {
    "description": "What this subsystem accomplishes",
    "deliverables": [
      "Concrete deliverable 1",
      "Concrete deliverable 2"
    ],
    "impact": "How this contributes to parent project"
  },

  "dates": {
    "start_date": "2025-01-01",
    "estimated_end_date": "2025-03-01",        // Usually has target end
    "actual_end_date": null
  },

  "estimates": {
    "total_days": 30,                          // Rough estimate
    "total_sessions": 20,
    "total_uptime_hours": 40,
    "avg_session_uptime_minutes": 120
  },

  "progress": {
    "phase": "Current phase",
    "current_focus": "Current task/area",
    "days_elapsed": 0,
    "sessions_completed": 0,
    "uptime_hours_actual": 0,
    "completion_percentage": 0
  },

  "milestones": [
    {
      "name": "Milestone 1",
      "target_date": "2025-02-01",
      "status": "pending",
      "description": "What this achieves"
    }
  ],

  "subprojects": [
    // IDs of low-level tasks under this
  ],

  "dependencies": [],                          // Other projects that must complete first

  "velocity": {
    "planned_sessions_per_week": 7,
    "actual_sessions_per_week": null,          // Calculated from session data
    "trend": null                              // on_track | ahead | behind | learning
  },

  "notes": [
    "Context, insights, observations"
  ],

  // ============================================================================
  // EXTENSIONS - Structured Discovery Space
  // ============================================================================
  // Fields discovered through living/working that don't fit standard structure yet
  // When validated broadly, promote to optional standard fields in next schema version

  "extensions": {
    // Discoveries live here until validated
  }
}
